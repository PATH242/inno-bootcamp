#include <bits/stdc++.h>
using namespace std;
int x[3],y[3],a[3];
int main ( ) {

  for(int i=0;i<3;i++)
    cin>>x[i]>>y[i];
  a[0]= sqrt( pow ( (x[1]-x[0]), 2 )  + pow(  (y[1]-y[0] ) , 2)  )  ;
  a[1]= sqrt( pow ( (x[2]-x[1]), 2 )  + pow(  (y[2]-y[1] ) , 2)  );
  a[2]= sqrt( pow ( (x[2]-x[0]), 2 )  + pow(  (y[2]-y[0] ) , 2) ) ;
  sort(a,a+3);
  if( a[0]+a[1] >= a[2] ) 
    cout<<"Those points can create a triangle\n";
  else
    cout<<"Those points can't create a triangle\n";
  return 0;
}
